{
  "name": "Top Menu Link - Create Issue",
  "description": "Atlassian Connect app",
  "key": "uk.tumakha.example.jira.plugin",
  "baseUrl": "https://tumakha.github.io",
  "vendor": {
    "name": "Yuriy Tumakha",
    "url": "https://tumakha.github.io"
  },
  "authentication": {
    "type": "none"
  },
  "apiVersion": 1,
  "modules": {
    "webItems": [
      {
        "location": "system.top.navigation.bar",
        "weight": 200,
        "conditions": [
          {
            "condition": "has_selected_project"
          }
        ],
        "url": "/secure/CreateIssue!default.jspa",
        "context": "product",
        "key": "create-issue",
        "name": {
          "value": "Create Issue"
        },
        "tooltip": {
          "value": "Create New Issue"
        },
        "styleClasses": [
          "webitem",
          "system-present-webitem"
        ]
      }
    ]
  }
}